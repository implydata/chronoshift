{
  "name": "chronoshift",
  "version": "0.4.4",
  "description": "A tiny library for shifting time with timezones",
  "keywords": [
    "date",
    "timezone",
    "duration",
    "time",
    "range",
    "IS08601",
    "8601"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/chronoshift.git"
  },
  "main": "build/chronoshift.js",
  "typescript": {
    "definition": "build/chronoshift.d.ts"
  },
  "scripts": {
    "test": "./run-tests",
    "prepublish": "./compile"
  },
  "config": {
    "blanket": {
      "pattern": "chronoshift/build/"
    }
  },
  "dependencies": {
    "immutable-class": "0.4.3"
  },
  "devDependencies": {
    "blanket": "1.2.3",
    "browserify": "13.0.0",
    "chai": "3.5.0",
    "event-stream": "3.3.2",
    "gulp": "3.9.1",
    "gulp-mocha": "2.2.0",
    "gulp-typescript": "2.12.1",
    "mocha": "2.4.5",
    "pegjs": "0.9.0",
    "run-sequence": "1.1.5",
    "tslint": "3.6.0",
    "typescript": "1.8.9"
  }
}
