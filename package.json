{
  "name": "chronoshift",
  "version": "0.8.1",
  "description": "A tiny library for shifting time with timezones",
  "keywords": [
    "date",
    "timezone",
    "duration",
    "time",
    "range",
    "IS08601",
    "8601"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/chronoshift.git"
  },
  "main": "build/chronoshift.js",
  "typings": "build/index.d.ts",
  "files": [
    "build/",
    "typings/"
  ],
  "scripts": {
    "prepare": "tdi -q",
    "test": "./travis-test",
    "compile": "./compile"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.s?css$": "identity-obj-proxy"
    },
    "testMatch": [
      "**/?(*.)+(spec).ts?(x)"
    ],
    "setupFilesAfterEnv": ["jest-expect-message"]
  },
  "dependencies": {
    "immutable-class": "~0.7.7",
    "moment-timezone": "~0.5.25",
    "tslib": "~1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/jest-expect-message": "^1.0.0",
    "@types/node": "^12.0.8",
    "immutable-class-tester": "0.6.0",
    "jest": "^24.8.0",
    "jest-expect-message": "^1.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.17.0",
    "typescript": "3.5.2"
  }
}
