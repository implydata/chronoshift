{
  "name": "chronoshift",
  "version": "0.8.1",
  "description": "A tiny library for shifting time with timezones",
  "keywords": [
    "date",
    "timezone",
    "duration",
    "time",
    "range",
    "IS08601",
    "8601"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/chronoshift.git"
  },
  "main": "build/chronoshift.js",
  "typings": "build/index.d.ts",
  "files": [
    "build/",
    "typings/"
  ],
  "scripts": {
    "prepare": "tdi -q",
    "test": "./travis-test",
    "compile": "./compile"
  },
  "config": {
    "blanket": {
      "pattern": "chronoshift/build/"
    }
  },
  "dependencies": {
    "immutable-class": "~0.7.0",
    "moment-timezone": "~0.5.21",
    "tslib": "~1.9.3"
  },
  "devDependencies": {
    "@types/mocha": "5.2.5",
    "babel-core": "6.26.3",
    "babel-preset-es2015": "6.24.1",
    "blanket": "1.2.3",
    "browserify": "16.2.2",
    "chai": "4.1.2",
    "gulp": "3.9.1",
    "gulp-mocha": "6.0.0",
    "gulp-typescript": "5.0.0-alpha.3",
    "immutable-class-tester": "0.6.0",
    "mocha": "5.2.0",
    "rollup": "0.66.0",
    "run-sequence": "2.2.1",
    "tdi": "0.6.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.0.3"
  }
}
