{
  "name": "chronoshift",
  "version": "0.4.6",
  "description": "A tiny library for shifting time with timezones",
  "keywords": [
    "date",
    "timezone",
    "duration",
    "time",
    "range",
    "IS08601",
    "8601"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/chronoshift.git"
  },
  "main": "build/chronoshift.js",
  "typescript": {
    "definition": "build/chronoshift.d.ts"
  },
  "scripts": {
    "test": "./run-tests",
    "prepublish": "./compile"
  },
  "config": {
    "blanket": {
      "pattern": "chronoshift/build/"
    }
  },
  "dependencies": {
    "@types/node": "6.0.31",
    "immutable-class": "0.4.6"
  },
  "devDependencies": {
    "@types/mocha": "2.2.29",
    "@types/chai": "3.4.30",
    "blanket": "1.2.3",
    "browserify": "13.0.1",
    "chai": "3.5.0",
    "event-stream": "3.3.3",
    "gulp": "3.9.1",
    "gulp-mocha": "2.2.0",
    "gulp-typescript": "2.13.6",
    "mocha": "2.5.3",
    "pegjs": "0.9.0",
    "run-sequence": "1.2.2",
    "tslint": "3.13.0",
    "typescript": "beta"
  }
}
