{
  "name": "chronoshift",
  "version": "0.6.10",
  "description": "A tiny library for shifting time with timezones",
  "keywords": [
    "date",
    "timezone",
    "duration",
    "time",
    "range",
    "IS08601",
    "8601"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/chronoshift.git"
  },
  "main": "build/chronoshift.js",
  "typings": "build/index.d.ts",
  "files": [
    "build/",
    "typings/"
  ],
  "scripts": {
    "prepare": "tdi -q",
    "test": "./travis-test",
    "compile": "./compile"
  },
  "config": {
    "blanket": {
      "pattern": "chronoshift/build/"
    }
  },
  "dependencies": {
    "immutable-class": "~0.6.42",
    "moment-timezone": "~0.5.14",
    "tslib": "~1.7.1"
  },
  "devDependencies": {
    "@types/mocha": "2.2.48",
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "blanket": "1.2.3",
    "browserify": "16.1.1",
    "chai": "4.1.2",
    "event-stream": "3.3.4",
    "gulp": "3.9.1",
    "gulp-mocha": "5.0.0",
    "gulp-typescript": "4.0.1",
    "immutable-class-tester": "0.5.12",
    "mocha": "5.0.4",
    "rollup": "0.56.5",
    "run-sequence": "2.2.1",
    "tdi": "0.5.2",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.5.3"
  }
}
