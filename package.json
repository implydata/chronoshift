{
  "name": "chronoshift",
  "version": "0.5.0",
  "description": "A tiny library for shifting time with timezones",
  "keywords": [
    "date",
    "timezone",
    "duration",
    "time",
    "range",
    "IS08601",
    "8601"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/chronoshift.git"
  },
  "main": "build/chronoshift.js",
  "typescript": {
    "definition": "build/chronoshift.d.ts"
  },
  "scripts": {
    "test": "./travis-test",
    "prepublish": "./compile"
  },
  "config": {
    "blanket": {
      "pattern": "chronoshift/build/"
    }
  },
  "dependencies": {
    "immutable-class": "0.5.2",
    "walltime-repack": "0.2.3"
  },
  "devDependencies": {
    "@types/chai": "3.4.30",
    "@types/mocha": "2.2.29",
    "babel-core": "6.13.2",
    "babel-preset-es2015": "6.13.2",
    "blanket": "1.2.3",
    "browserify": "13.1.0",
    "chai": "3.5.0",
    "dts-bundle": "0.5.0",
    "event-stream": "3.3.4",
    "gulp": "3.9.1",
    "gulp-mocha": "3.0.0",
    "gulp-typescript": "2.13.6",
    "mocha": "3.0.1",
    "pegjs": "0.9.0",
    "rollup": "0.34.7",
    "run-sequence": "1.2.2",
    "tslint": "3.14.0",
    "typescript": "beta"
  }
}
