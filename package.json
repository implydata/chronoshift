{
  "name": "chronoshift",
  "version": "0.6.7",
  "description": "A tiny library for shifting time with timezones",
  "keywords": [
    "date",
    "timezone",
    "duration",
    "time",
    "range",
    "IS08601",
    "8601"
  ],
  "author": {
    "name": "Vadim Ogievetsky",
    "url": "http://vadim.ogievetsky.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "http://github.com/implydata/chronoshift.git"
  },
  "main": "build/chronoshift.js",
  "typings": "build/index.d.ts",
  "files": [
    "build/",
    "typings/"
  ],
  "scripts": {
    "prepare": "tdi -q",
    "test": "./travis-test",
    "compile": "./compile"
  },
  "config": {
    "blanket": {
      "pattern": "chronoshift/build/"
    }
  },
  "dependencies": {
    "immutable-class": "~0.6.39",
    "moment-timezone": "~0.5.13",
    "tslib": "~1.7.1"
  },
  "devDependencies": {
    "@types/mocha": "2.2.43",
    "babel-core": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "blanket": "1.2.3",
    "browserify": "14.4.0",
    "chai": "4.1.2",
    "event-stream": "3.3.4",
    "gulp": "3.9.1",
    "gulp-mocha": "4.3.1",
    "gulp-typescript": "3.2.2",
    "immutable-class-tester": "0.5.12",
    "mocha": "4.0.1",
    "rollup": "0.50.0",
    "run-sequence": "2.2.0",
    "tdi": "0.3.16",
    "ts-node": "^3.3.0",
    "tslint": "5.7.0",
    "typescript": "2.5.3"
  }
}
