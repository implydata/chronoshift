    "chai": "3.5.0",
    "dts-bundle": "0.5.0",
    "event-stream": "3.3.4",
    "gulp": "3.9.1",
    "gulp-mocha": "3.0.1",
    "gulp-typescript": "2.13.6",
    "immutable-class-tester": "0.5.3",
    "mocha": "3.0.2",
